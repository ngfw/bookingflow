parameters:
    level: 1
    paths:
        - app
    ignoreErrors:
        - '#Call to an undefined static method App\\Models\\.*::(where|find|create|orderBy|whereHas|count|sum|pluck|get|first|exists|when|withCount|selectRaw)#'
        - '#Call to an undefined static method Illuminate\\Support\\Facades\\.*::#'
        - '#Access to an undefined property App\\Models\\.*::\$.*#'
        - '#Anonymous function has an unused use#'